#include<iostream>
using namespace std;

inline int addNumbers(int a, int b){
    int c=a+b;
    return c;
}

int multiplyNumbers(int x, int y, int z = 1) {
    return x*y*z;
}

int main() {
    int sum = addNumbers(5, 7);
    cout<<sum<<endl;
    int product = multiplyNumbers(3, 4); 
    cout<<product<<endl;// Use default argument
    return 0;
}
